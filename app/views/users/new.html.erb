<h1>User Sign Up Page</h1> 
<%= form_for @user do |f| %>
<%= f.label :username %>
<%= f.text_field :username %>
<br>
<%= f.label :password %>
<%= f.password_field :password %>
<br> 
<%= f.label :first_name %>
<%= f.text_field :first_name %>
<br> 
<%= f.label :last_name %>
<%= f.text_field :last_name %>
<%= f.submit %> 

<% end %>